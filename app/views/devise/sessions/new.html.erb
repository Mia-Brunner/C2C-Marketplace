<div class="new-user-page">
  <div class="edit-left-image"></div>
  <div class="editright">
<h2>LOG IN</h2>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  
  <br>
  <div class="moverightplease">
  <div class="form-group row">
  </div>
  </div>
  
  <div class="form-group row">
    <%= f.label :email, class: 'col-sm-3 col-form-label' %><br />
      <div class="col-sm-9">
    <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
 </div>
  </div>

  <div class="form-group row">
    <%= f.label :password, class: 'col-sm-3 col-form-label' %><br />
      <div class="col-sm-9">
    <%= f.password_field :password, autocomplete: "current-password" %>
 </div>
  </div>

  <% if devise_mapping.rememberable? %>
    <div class="form-group row">
      <div class="col-sm-9">
      <%= f.label :remember_me %>
      <%= f.check_box :remember_me, class: 'col-sm-3 col-form-label' %>
      </div>
    </div>
  <% end %>

  <div class="col-sm-3"></div>
    <div class="editsubmit">
    <%= f.submit "Log in", class:'submitedit' %>
  </div>
<% end %>
<br>
<br>
<%= render "devise/shared/links" %>

  </div>
 </div>
</div>
